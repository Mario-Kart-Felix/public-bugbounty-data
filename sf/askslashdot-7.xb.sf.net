        function checkCloseAd(){
            var adon = $('div.ad-on');
            if(adon){
                var closead = adon.find('.close-ad');
                if(!closead || closead.length === 0){
                    adon.prepend('<div class="close-ad"><span>Close Ad</span></div>');
                }
            }
        }
        function footerFixer(){
            var footer = $('#home .stage-center #footer');
            if(footer.length){
                if(footer.length > 1){
                    $('#home .stage-center #footer').eq(1).remove();
                }
                if(footerHtml != $(footer).html()) footerHtml = $(footer).html();
            }else{
                $('#home .stage-center div').first().append('<div id="footer">'+footerHtml+'</div>');
            }
        }
        $('#river_bottom').prepend('<div class="close-ad"><span>Close Ad</span></div>');
        $('body').on('click','.close-ad', function(){
            $(this).hide();
            $('#river_bottom,#story_slot2').toggleClass('ad-on');
        });
        setInterval(checkCloseAd,1000);
        setInterval(footerFixer,1000);
    });
</script>

