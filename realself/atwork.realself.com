<script type='text/javascript'>(function($) {
    
    var $btn = $('nav.rs-subNav .rs-moreBtn');
    var $hlinks = $('nav.rs-subNav .rs-expand');

    function calcWidth() {

        var navwidth = 0;
        var morewidth = $('.rs-subNavList .rs-more').outerWidth(true);
        $('.rs-subNavList > li:not(.rs-more)').each(function() {
            navwidth += $(this).outerWidth( true );
        });
        var availablespace = $('nav.rs-subNav').outerWidth(true) - morewidth;
        
        if (navwidth > availablespace) {
          var lastItem = $('.rs-subNavList > li:not(.rs-more)').last();
          lastItem.attr('data-width', lastItem.outerWidth(true));
          lastItem.prependTo($('.rs-subNavList .rs-more ul'));
          calcWidth();
        } else {
          var firstMoreElement = $('.rs-subNavList li.rs-more li').first();
          if (navwidth + firstMoreElement.data('width') < availablespace) {
            firstMoreElement.insertBefore($('.rs-subNavList .rs-more'));
          }
        }
        
        if ($('.rs-more li').length > 0) {
          $('.rs-more').css('display','inline-block');
        } else {
          $('.rs-more').css('display','none');
        }
       
    }
    
    $(window).resize(function() {
        calcWidth();
    });

    // toggle drop down menu
    $btn.on('click', function() {
        $hlinks.toggleClass('rs-show');
    });
    
    calcWidth();
    
    // add active class by url
    str1 = this.location.pathname.replace(/\/+$/,'') + '/';
    str2 = this.location.pathname.replace(/\/+$/,'');
    $('nav.rs-subNav a[href="' + str1 + '"], a[href="' + str2 + '"]').addClass('active');
  
})( jQuery );</script>
