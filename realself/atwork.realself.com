    // these functions come from the theme, rather than the page.
		
    // Get query Parameter
    function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function setTitle(title) {
        document.querySelector(".job-description-title").innerHTML = title;
    }

    function setContent(content) {
        document.querySelector(".job-description-content").innerHTML = content;
    }
        
    function setApplyLink(jobId) {
        var applyLink = "https://atwork.realself.com/careers/apply/?gh_jid=" + jobId;
        var applicantSource = getParameterByName("gh_src");
        if (applicantSource) {
            applyLink += "&gh_src=" + applicantSource;
        }

        document.querySelector(".job-description-apply-link").href = applyLink;
    }

    function showJob() {
        var container = document.querySelector(".job-description-container");
        if (container) {
            container.classList.remove("u-hidden");
        }
    }

    function showApply() {
        document.querySelector(".job-description-apply").classList.remove("u-hidden");
    }

    function desanitizeHtml(escapedHtml) {
        return escapedHtml.replace(/&lt;/gi, "<").replace(/&gt;/gi, ">").replace(/&quot;/gi, "\"").replace(/&apos;/gi, "'").replace(/&amp;/gi, "&");
    }

    function populateJob(jobJson) {
        var title = jobJson.title;
        var content = desanitizeHtml(jobJson.content);
        var jobId = jobJson.id;
        
        setTitle(title);
        setContent(content);
        setApplyLink(jobId);
        
    }

    function retrieveJob(jobId, callback) {
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function() {
            if (this.readyState == 4 && this.status == 200) {
                var jobJson = JSON.parse(this.responseText);
                populateJob(jobJson);
                callback();
            }
        };
        xhttp.open("GET", "https://api.greenhouse.io/v1/boards/realself/jobs/" + jobId, true);
        xhttp.send();
    }

    function loadJob() {
        var jobId = getParameterByName("gh_jid");
        
        if(jobId) {
            retrieveJob(jobId, function() { showApply(); showJob(); } );
        } else {
            showJob();
            
        }
    }
    window.addEventListener("load", loadJob);
</script>
