  }
  .social .sub-header {
  	margin-top: -70px;
  }
  .social .sub-header .icon {
  	background-color: #000;
    color: #FFF;
    -webkit-border-radius: 80px;
    border-radius: 80px;
    font-size: 40px;
    text-align: center;
    display: block;
    width: 80px;
    height: 80px;
    padding-top: 20px;
    overflow: hidden;
    margin: 0 auto;
  }
  .social .sub-header .handle {
  	text-transform: uppercase;
    text-align: center;
    margin-top: 15px;
    margin-bottom: 40px;
    color: #000;
  
  }
  .social .sub-header .handle a {
  	  font-weight: bold;
      color: #ffffff;
  }
  .social .sub-header .handle a:hover {
  	color: #000000;
  }
  .social .row .twitter {
  	border-right: 1px solid rgba(0,0,0,.1);
  }
  .social .row .twittersolo {
  	border-right: none;
  }
  
  .social .twitter li {
  	border-bottom: 1px solid rgba(0,0,0,.1);
    padding-bottom: 40px;
    margin-bottom: 40px;
    color: #ffffff;
  }
  .social .twitter li:last-child {
  	border-bottom: none;
  }
  .social .post {
  	font-size: 16px;
    line-height: 20px;
    margin-bottom: 6px;
  }
  .social .post a {
  	text-decoration: none;
    color: #ffffff;
  }
  .social .post a:hover {
  	text-decoration: underline;
    color: #000000;
  }
  .social .meta {
  	text-transform: uppercase;
    font-size: 14px;
  }
  .social .meta a {
  	color: rgba(255,255,255,.4);
    text-decoration: none;
  }
  .social .meta a:hover {
  	color: rgba(255,255,255,1);
  }
  .social .twitter .media {
  	float: right; 
  }
  .social .twitter .media a {
  	  width: 70px;
    height: 70px;
    display: block;
    -webkit-border-radius: 70px;
    border-radius: 70px;
    overflow: hidden;
    -webkit-transition: all .1s ease-in;
    -moz-transition: all .1s ease-in;
    -ms-transition: all .1s ease-in;
    -o-transition: all .1s ease-in;
    transition: all .1s ease-in;
  }
  .social .twitter .media a:hover {
  	-moz-transform: scale(1.2) rotate(0deg) translateX(0px) translateY(0px) skewX(0deg) skewY(0deg);
    -webkit-transform: scale(1.2) rotate(0deg) translateX(0px) translateY(0px) skewX(0deg) skewY(0deg);
    -o-transform: scale(1.2) rotate(0deg) translateX(0px) translateY(0px) skewX(0deg) skewY(0deg);
    -ms-transform: scale(1.2) rotate(0deg) translateX(0px) translateY(0px) skewX(0deg) skewY(0deg);
    transform: scale(1.2) rotate(0deg) translateX(0px) translateY(0px) skewX(0deg) skewY(0deg);
  }
  .social .tumblrsolo {
  	padding-bottom: 40px;
  }
  .social .tumblr .entry {
  	float: left;
    width: 165px;
    height: 165px;
    margin-right: 2px;
    margin-bottom: 2px;
  }
  .social .tumblr .entry.photo a {
  	display: block;
    text-indent: -9999px;
  	width: 165px;
    height: 165px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;
    opacity: .5;
    -webkit-transition: all .1s ease-in;
    -moz-transition: all .1s ease-in;
    -ms-transition: all .1s ease-in;
    -o-transition: all .1s ease-in;
    transition: all .1s ease-in;
  }
  .social .tumblr .entry.photo a:hover {
  	opacity: 1;
  }
  .social .tumblr .entry.photo .caption {
  	display: none;
  }
  
  @media only screen and (min-width: 40.063em) and (max-width: 64em) { 
  	.social .padding {
    	padding-left: 20px;
    }
  	.social .tumblr .entry {
    	width: 49%;
      height: 49%;
      min-height: 200px;
    }
    .social .tumblr .entry:nth-child(4n+1) {
    	display: none;
    }
    .social .tumblr .entry.photo a{
    	width: 100%;
      height: 100%;
      min-height: 200px;
    }
  
  }
  @media only screen and (max-width: 40em) { 
  	.social .twitter .post {
    	font-size: 13px;
    }
    .social .twitter .meta {
    	font-size: 11px;  
    }
  	.social .tumblr {
  		padding-bottom: 40px;
  	}
  	.social .tumblr .entry {
    	width: 32%;
      height: 32%;
      min-height: 100px;
    }
    .social .tumblr .entry.photo a{
    	width: 100%;
      height: 100%;
      min-height: 100px;
    }
  }
  
  
