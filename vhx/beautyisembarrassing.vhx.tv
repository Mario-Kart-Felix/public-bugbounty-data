              url: url,
              reply_url: reply_url,
              retweet_url: retweet_url,
              favorite_url: favorite_url,
              tweet: status
            });
          }
        }

        $('#twitter_' + this.username).html(Mustache.to_html($('#vhx_twitter_partial').html(), posts));
      }
