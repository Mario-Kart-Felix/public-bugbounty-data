      renderTweets: function(twitters) {
        var posts = { posts: [] };

        if (twitters && twitters.length) {
          for (var i = 0; i < twitters.length; i++) {
            var username = twitters[i].user.screen_name;

            var status = twitters[i].text.replace(/((https?|s?ftp|ssh)\:\/\/[^"\s\<\>]*[^.,;'">\:\s\<\>\)\]\!])/g, function(url) {
              return '<a href="' + url + '" target="_blank">' + url + '</a>';
            }).replace(/\B@([_a-z0-9]+)/ig, function(reply) {
                return '<a href="http://twitter.com/' + reply.substring(1) + '" target="_blank">@' + reply.substring(1) + '</a>';
              }).replace(/\#([_a-z0-9+\-]+)/ig, function(hash) {
                return '<a class="tag" href="http://twitter.com/search/%23' + hash.substring(1) + '" target="_blank">' + hash + '</a>';
              });

            var cls = i == twitters.length - 1 ? ' last' : '';
            var url = 'http://twitter.com/' + username + '/statuses/' + twitters[i].id_str;
            var reply_url = 'https://twitter.com/intent/tweet?in_reply_to=' + twitters[i].id_str;
            var retweet_url = 'https://twitter.com/intent/retweet?tweet_id=' + twitters[i].id_str;
            var favorite_url = 'https://twitter.com/intent/favorite?tweet_id='  + twitters[i].id_str;

            posts.posts.push({
