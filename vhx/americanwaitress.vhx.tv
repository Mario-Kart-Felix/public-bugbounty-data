	          e.preventDefault();
	  
	          $("nav .toggle a").removeClass("on").addClass("off");
	          $(this).addClass("on");
	  
	          var rel = $(this).attr("rel");
	          var $targ = $(".pages .page[rel='"+rel+"']");
	  
	          $targ.addClass('is-active');
	          $(".pages .page").not("[rel='"+rel+"']").removeClass('is-active');
