    console.log('initModelSelector();', manufacturerId, manufacturerType);
    models = false;
    modelgroups = false;
    series = false;
    variants = false;
    $('#modell').find('option').remove();
    $('#modell').append('<option value="" selected>Alle<\/option>');

    if (manufacturerId > 0) {
        if (manufacturersWithSeries.indexOf(manufacturerId) > -1 && manufacturerType !== "MOT" && manufacturerType !== "NFZ") {
            console.log('manufacturersWithSeries');

            $.getJSON(baseUri + '/criteria/series', {
                manufacturers: $('#hersteller').val().split('_')[0],
                culture: culture,
                apikey: apikey
            }, function (resultmodels) {
                resultmodels = resultmodels.filter(function (item) {
                    return item.isActive || true;
                });
                if (resultmodels.length > 0) {
                    series = true;
                    seriesList = [];
                    for (var i in resultmodels) {
                        $('#modell').append('<option value="' + resultmodels[i].value + '">' + resultmodels[i].label + '<\/option>');
                        seriesList.push(resultmodels[i].label );
                    }
                    queryCount();


                } else {
                    variants = true;
                    $.getJSON(baseUri + '/criteria/variants', {
                        manufacturers: $('#hersteller').val().split('_')[0],
                        culture: culture,
                        apikey: apikey
                    }, function (resultmodels) {
                        resultmodels = resultmodels.filter(function (item) {
                            return item.isActive || true;
                        });
                        //$('#modell').find('option').remove();
                        //$('#modell').append('<option value="" selected>Alle<\/option>');
                        for (var i in resultmodels) {
                            $('#modell').append('<option value="' + resultmodels[i].value + '">' + resultmodels[i].label + '<\/option>');
                        }
                    });
                }


                makeSelect($('#modell'));
            });

        } else {
            $.getJSON(baseUri + '/criteria/models', {
                manufacturers: $('#hersteller').val().split('_')[0],
                culture: culture,
                apikey: apikey
            }, function (resultmodels) {
                modelgroups = false;
                console.log(makeSelect, resultmodels);
                resultmodels = resultmodels.filter(function (item) {
                    return item.isActive;
                });
                $.getJSON(baseUri + '/criteria/modelgroups', {
                    manufacturers: $('#hersteller').val().split('_')[0],
                    culture: culture,
                    apikey: apikey
                    //modelgroups: [+$('#hersteller').val().split('_').toLowerCase()]
                }, function (result) {
                    if (result.length > 0) {
                        console.log('manufacturersWithModelgroups');
                        modelgroups = true;
                        series = false;
                        $('#modell').find('option').remove();
                        $('#modell').append('<option value="" selected>Alle<\/option>');
                        for (var group in result) {
                            var x=result[group].grouptype;
                            if(!result[group].groupType || manufacturerType === 'ALL' || result[group].groupType === manufacturerType  ){
                                var resultmodelgroups = result[group].items;
                                //if (result.length > 1) $('#modell').append('<optgroup  label="' + result[group].label + '">');

                                for (var i in resultmodelgroups) {
                                    $('#modell').append('<option value="' + resultmodelgroups[i].value + '">' + resultmodelgroups[i].label + '<\/option>');
                                }

                                if (result.length > 1) $('#modell').append('<\/optgroup>');
                            }

                        }
                    } else {
                        modelgroups = false;
                        models = true;
                        for (var i in resultmodels) {
                            $('#modell').append('<option value="' + resultmodels[i].value + '">' + resultmodels[i].label + '<\/option>');
                        }
                        makeSelect($('#modell'));
                    }

                });

            });

        }

    }
}

