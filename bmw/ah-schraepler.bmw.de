function queryCount() {
    console.log('queryCount()');
    if (countXhr) {
        countXhr.abort();
    }

    var usagetypes = $('#fahrzeugart').val();
    var bodies = $('#karosserie').val();
    var bodyGroups = $('#fahrzeugtyp').val();

    var selection = {
        zip: '',
        distance: '',

        usageTypes: [],
        manufacturers: [],
        manufacturersType: $('#hersteller').val() ? $('#hersteller').val().split('_')[1] : [],
        models: '',

        bodies: '',
        bodyGroups: '',

        fuellings: '',
        transmission: '',
        powerKwMin: '',
        powerKwMax: '',

        paints: [],
        metallic: false,
        priceMin: '',
        priceMax: '',

        registermin: null,
        registermax: null,

        vatdeductible: false,
    };

    var args = {
        zip: $('#plz').val(),
        distance: $('#umkreis').val(),

        usageTypes: $('#fahrzeugart').prop('selectedIndex') > 0 ? $('#fahrzeugart').val() : null,
        bodyGroups: $('#fahrzeugtyp').prop('selectedIndex') > 0 ? [ $('#fahrzeugtyp').val() ] : null,

        bodies: $('#karosserie').prop('selectedIndex') > 0 ? $('#karosserie').val() : null,
        manufacturers: $('#hersteller').val() ? $('#hersteller').val().split('_')[0] : [],

        manufacturersType: $('#hersteller').val() ? $('#hersteller').val().split('_')[1] : [],

       models: $('#modell').prop('selectedIndex') > 0 ? [$('#modell').val()] : null,

        fuellings: $('#kraftstoff').val(),
        transmission: $('#getriebe').val(),
        powerKwMin: $('#slider-leistung-quicksearch-value-lower').val(),
        powerKwMax: $('#slider-leistung-quicksearch-value-upper').val(),

        paints: $('#farbe').val(),
        metallic: $('#metallic').prop('checked') ? true : null,

        priceMin: $('#preismin').val() > 0 ? $('#preismin').val() : null,
        priceMax: $('#preismax').val() > 0 ? $('#preismax').val() : null,

        registerMin: $('#zulassungvon').val() > 0 ? $('#zulassungvon').val() : null,
        registerMax: $('#zulassungbis').val() > 0 ? $('#zulassungbis').val() : null,

        mileageMax: $('#kilometerbis').val() > 0 ? $('#kilometerbis').val() : null,

        vatdeductible: $('#mwstausweisbar').prop('checked') ? true : null,
    };
    args.models = null;
    args.series = null;
    args.variants = null;
    if(!!manufacturers ){
        if (modelgroups) {
            args.modelGroups = $('#modell').prop('selectedIndex') > 0 ? [$('#modell').val()] : null;
        } else if (series) {
            args.series = $('#modell').prop('selectedIndex') > 0 ? [$('#modell').val()] : null;

            if (!!args.series  && typeof args.series == "object"){
                args.series = args.series.join(',');
                args.series = args.series.replace(/"/g,'');
                args.series = args.series.replace(/"/g,'');
            }

        } else if (variants) {
            args.variants = $('#modell').prop('selectedIndex') > 0 ? [$('#modell').val()] : null;
        } else {
            args.models = $('#modell').prop('selectedIndex') > 0 ? [$('#modell').val()] : null;
        }
    }

    $('#carsearchlink').attr('href', settings.url + '#!' + target + '/?' + getListPageQuery(args));
    if(settings.newWindow){
        $('#carsearchlink').attr('target', '_blank');
    }

    if(settings.debug) $('#output').html(settings.url + '#!' + target + '/?' + getListPageQuery(args));
    countXhr = $.getJSON(baseUri + '/vehicles/count/?' + getCountQuery(args), function (result) {
        $('.quicksearch-count').text(result);
    });
}

function getListPageQuery(selection) {

    var q = [];

    for (var fld in selection) {
        if (selection[fld] && !$.isFunction(selection[fld])) {
            var v = selection[fld];
            if(fld === 'manufacturersType'){
                if(v.length > 0){
                    q.push(fld + '=' + v);
                }
            }else if(fld === 'series'){
                q.push(fld + '=' + v);
            }else if ($.isNumeric(v)) {
                q.push(fld + '=' + parseFloat(v));
            } else if (typeof v === "string") {
                q.push(fld + '="' + v + '"');
            } else if (!$.isArray(v) || v.length > 0) {
                var result = JSON.stringify(v).substring(1);
                result = result.substring(0, result.length - 1);
                q.push(fld + '=' + result);
            }
        }
    }

    for (var i in q) {
        if (!q[i]) {
            delete q[i];
        }
    }

    return q.join('&');
}

function getCountQuery(selection) {
    var manufacturerId = 0
    var manufacturerType = '';
    var manufacturerLabel = '';
    if(!!$('#hersteller').val()){
        manufacturerId = parseInt($('#hersteller').val().split('_')[0]);
        manufacturerType = $('#hersteller').val().split('_')[1] || 'ALL';
    }
    manufacturers.forEach(function(man){
        if(man.value == manufacturerId) manufacturerLabel = man.label;
    });

    var q = [];
    for (var fld in selection) {
        if (selection[fld] && !$.isFunction(selection[fld])) {
            if(fld == 'manufacturersType' && manufacturerType !== 'ALL' ){
                //toQuery(q, 'bodies', getBodies());
                if(!!manufacturerId ) {
                    if (manufacturerType === 'PKW' && manufacturersWithSeries.indexOf(manufacturerId) > -1) {
                        if (selection.series) {
                            toQuery(q, 'series', selection.series);
                        } else {
                            var x = seriesList;
                            toQuery(q, 'models', seriesList);
                        }

                    } else {
                        if (!selection.models && !selection.modelGroups ) {
                            toQuery(q, 'models', (manufacturerLabel + '_' + manufacturerType).toLowerCase());
                        } else {
                            toQuery(q, 'models', selection.models);
                        }
                    }
                }

            }
            if (modelgroups && fld == 'modelGroups') {
                toQuery(q, 'models', selection[fld]);
            }else if (modelgroups && fld == 'models') {
                toQuery(q, 'models', selection[fld]);
            } else if (series && fld == 'series' ) {

                toQuery(q, 'series', selection[fld]);
            } else  {
                toQuery(q, fld, selection[fld]);
            }

        }
    }

    for (var i in q) {
        if (!q[i]) {
            delete q[i];
        }
    }
    toQuery(q, 'apikey', apikey);
    return q.join('&');
}

function toQuery(q, fld, v) {

    if ($.isArray(v)) {

        for (var i in v) {
            toQuery(q, fld, v[i]);
        }

    } else {
        if ($.isNumeric(v)) {
            q.push(fld + '=' + parseFloat(v));
        } else if (typeof v === "string") {
            q.push(fld + '=' + v);
        } else {
            q.push(fld + '=' + JSON.stringify(v));
        }
    }
}

quicksearch = {};
