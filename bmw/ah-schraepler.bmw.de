                makeSelect($('#fahrzeugtyp'));
            }
            initSelectors();

        });
    });
}

function getManufacturerById(id){
    var obj = manufacturers.filter(function(obj){
        if(obj.value == id ){
            return true;
        }
    })[0];
    if(!!obj){
        return obj;
    }else{
        return { label: ''}
    }
}
function isFixedPKW(id){
    for(var i=0;i<fixedManufacturers.length;+i++){
        if(fixedManufacturers[i]==id && fixedManufacturersModelGroupTypes[i] === "PKW"){
            return false;
        }
    }
    return true;
}
