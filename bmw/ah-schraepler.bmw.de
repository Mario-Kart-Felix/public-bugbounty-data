        makeSelect($('#fahrzeugart'));
    });
    $.getJSON(baseUri + '/criteria/bodies', { culture: culture, apikey: apikey }, function(result) {
        result = result.filter(function(item){
            return item.isActive || true;
        });
        $('#karosserie').append('<option value="" selected>Alle<\/option>');
        for (var i in result) { $('#karosserie').append('<option value="' + result[i].value + '">' + result[i].label + '<\/option>'); }
        makeSelect($('#karosserie'));
    });

    $('#hersteller').find('option').remove();
    $('#hersteller').append('<option value="" selected>Alle<\/option>');
    for(var i=0; i< fixedManufacturers.length;i++){
        var value = "";
        var label = "";
        if(fixedManufacturersModelGroupTypes[i] === "PKW"){
            value= fixedManufacturers[i]+"_"+fixedManufacturersModelGroupTypes[i];
            label= getManufacturerById(fixedManufacturers[i]).label + "";
        }else if(fixedManufacturersModelGroupTypes[i] === "MOT"){
            value= fixedManufacturers[i]+"_"+fixedManufacturersModelGroupTypes[i];
            label= getManufacturerById(fixedManufacturers[i]).label + " Motorrad";
        }else if(fixedManufacturersModelGroupTypes[i] === "NFZ"){
            value= fixedManufacturers[i]+"_"+fixedManufacturersModelGroupTypes[i];
            label= getManufacturerById(fixedManufacturers[i]).label + " Nutzfahrzeuge";
        }else{
            value=fixedManufacturers[i]+"_"+"ALL";
            label= getManufacturerById(fixedManufacturers[i]).label;
        }
        $('#hersteller').append('<option value="'+value+'">'+label+'<\/option>');
    }
    //$('#hersteller').append('<option value="" selected>------------------------------------<\/option>');
    $.getJSON(baseUri + '/criteria/manufacturers', { culture: culture, apikey: apikey }, function(result) {
        result = result.filter(function(item){
            return item.isActive && isFixedPKW(item.value);
        });
        for (var i in result) { $('#hersteller').append('<option value="' + result[i].value + '">' + result[i].label + '<\/option>'); }
        makeSelect($('#hersteller'));
    });

    $('#kraftstoff').find('option').remove();
