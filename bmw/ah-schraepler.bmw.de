        for (var i in result) { $('#kraftstoff').append('<option value="' + result[i].value + '">' + result[i].label + '<\/option>'); }
        makeSelect($('#kraftstoff'));
    });
}
var manufacturerId = 0;
var manufacturerType = '';
function initModelSelector() {
