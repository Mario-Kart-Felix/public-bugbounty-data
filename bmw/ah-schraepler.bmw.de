function initButtons() {
    console.log("Quicksearch marketplace 4.0 template 1.1.2");
    var buttons = $('.quick-search-button');
    var tabs = $('.quick-search-body');

    buttons.click(function() {
        var buttonIndex = buttons.index(this);
        var tabIndex = tabs.filter(':visible').index();
        if (tabs.is(':visible') === true && (buttonIndex + 1) != tabIndex) {
            tabs.hide();
            tabs.eq(buttonIndex).fadeIn();
        } else {
            tabs.eq(buttonIndex).slideToggle();
        }
    });
}
function loadConfig(){

    $.getJSON(baseUri + '/uiconfig/all', { culture: culture, apikey: apikey }, function(result) {
        if(!!result.searchForm && !!result.searchForm.manufacturersWithSeries){
            manufacturersWithSeries = result.searchForm.manufacturersWithSeries;
        }
        fixedManufacturers = result.searchForm.fixedManufacturers || [];
        fixedManufacturersModelGroupTypes = result.searchForm.fixedManufacturersModelGroupTypes || [];



        console.info('manufacturersWithSeries ',manufacturersWithSeries);
        console.info('fixedManufacturers ',fixedManufacturers);
        console.info('fixedManufacturersModelGroupTypes ',fixedManufacturersModelGroupTypes);
        $.getJSON(baseUri + '/criteria/all', { culture: culture, apikey: apikey }, function(result) {
            manufacturers = result.manufacturers;
            bodies = result.bodies;
            bodiesPKW = bodies.filter(function(obj){
                return bodiesMOT.indexOf(obj.value) === -1 && bodiesNFZ.indexOf(obj.value) === -1;
            });
            var bodyGroups = [];
            $('#fahrzeugtyp').append('<option value="">Alle<\/option>');
            if(!!result.bodyGroups) {
                bodyGroups = result.bodyGroups.filter(function(item){
                    return !(item.isActive === false);
