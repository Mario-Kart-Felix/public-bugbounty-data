<style>
.hidden {
    visibility: hidden;
}

.no-display {
    display: none;
}

#aa-install-widget-desktop {
    width: 100%;
    text-align: center;
}
.aa-install-desktop {
    min-width: 400px;
    height: 80px;
}
.aa-install-button-content {
    width: 100%;
    height: 100%;
    display: block;
    position: relative;
    text-align: center;
    font-size: 20px;
}
.aa-install-desktop .a-button-inner {
    height: 100%;
}
.aa-install-desktop .a-button-text {
    height: 100%;
}

#aa-widget-already-installed {
    min-width: 400px;
    height: 80px;
}
#aa-widget-already-installed .a-button-inner {
    height: 100%;
}
#aa-widget-already-installed .a-button-inner .a-button-text {
    position: relative;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
    top: 50%;
}

.aa-install-desktop .aa-browser-icon {
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
    top: 50%;
    padding: 0px 40px 0px 65px;
}

.aa-browser-icon {
    background: url("https://m.media-amazon.com/images/G/01/associates/BIT/AA-Browser-Icon-Sprite._CB420083502_.png") no-repeat;
    display: block;
    position: relative;
    left: 18px;
    height: 50px;
    line-height: 50px;
