<script>
(function() {
    P.when("A", "ready").execute(function (A) {
        /**
         * Endpoint for the Morgana local storage writer page which is loaded into an
         * iframe. This iframe will use the query parameters passed from here to set
         * local storage values that are used in the extension to set up attribution.
         */
        var localStorageWriterPath = "https://www.amazon.com/gp/ubp/misc/crossDomainLocalStorageWriter.html";

        /* Campaign code to use for attribution */
        var campaignCode = "org";

        /* Campaign code to use for attribution */
        var tagbase = "amazon1";

        /**
         * Bit mode used to detect smile mode for installations.
         * This is a legacy feature and was included in case it was needed
         * by some users or used in attribution. If we are able to confirm
         * that this is not used for attribution then this should be safe
         * to remove, since the extension automatically enables smile mode.
         */
        var bitMode = "";

        /**
         * Associate ID used in the legacy 1BA app. This was included in
         * case it was still being used by some users today for attribution,
         * but if we can confirm it is not used then it is safe to remove.
         */
        var associateId = "";

        var $ = A.$;

        /**
         * Sends a request to write to amazon.com local storage.
         * This value is picked up by the extension after it is installed in order to
         * update its programCode. This is then sent to our Redshift tables and is used
         * in conjunction with ref tag hits to calculate partner attribution.
         */
        function writeToUSLocalStorage() {
            var storageWriterPath = localStorageWriterPath + "?";

            if (campaignCode) {
                storageWriterPath += "programCode=" + campaignCode + "&";
            }
            if (tagbase) {
                storageWriterPath += "tagbase=" + tagbase + "&";
            }
            if (bitMode) {
                storageWriterPath += "bitMode=" + bitMode + "&";
            }
            if (associateId) {
                storageWriterPath += "associateId=" + associateId + "&";
            }

            var iframe = document.createElement('iframe');

            iframe.src = storageWriterPath;
            iframe.style.display = "none";

            document.body.appendChild(iframe);
        }

        writeToUSLocalStorage();
    });
})();
</script>

