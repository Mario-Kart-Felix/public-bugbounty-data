<!-- Begin AA installation detection -->


<script>
/**
 * Utility methods needed for event handler logic.
 */
var aaPageUtilities = (function() {
    /**
     * Runs action just before the next repaint.  This protects the
     * action from running out of sequence of the browser frame rate and causing "jank".
     * @param action {Function}
     * @returns {void}
     */
    var runOnRepaint = function(action) {
        typeof action === "function" && window.requestAnimationFrame(action);
    }

    /**
     * Gets the current user pixel position.
     * @param doc {document}
     * @returns {number}
     */
    var currentPosition = function(doc) {
        var position;
        try {
            position = window.pageYOffset || doc.documentElement.scrollTop || doc.body.scrollTop || 0;
        } catch(e) {
            position = 0;
        }
        return position;
    };

    return {
        runOnRepaint: runOnRepaint,
        currentPosition: currentPosition
    };
}());
</script>


