<!-- Script for checking installation of the desktop extension -->
<script>
var aaExtensionDetection = (function() {
    /* Boolean that will be set to true upon receiving a UBP message from AA */
    var aaInstalled = false;

    /* Number of times the workflow will execute before deciding that the extension is not installed */
    var aaDetectionRetries = 4;

    /* Interval to wait between checking for extension installation */
    var aaDetectionInterval = 1000;

    /**
     * Simple UUID generation for creating UBP messages
     * @returns {string}
     */
    function uuid() {
        var S4 = function() {
            return Math.floor(Math.random() * 0x10000 /* 65536 */
            ).toString(16);
        };
        return (S4() + S4() + "-" + S4() + "-" + S4() + "-" + S4() + "-" + S4() + S4() + S4());
    }

    /*
     * Creates a simple API call to the extension. To see how this is handled see the following code:
     * https://code.amazon.com/packages/UBPExtension/blobs/mainline/--/src/ubp/extension/platform/api/platform/PlatformApiStrategy.ts
     *
     * @returns {object}
     */
    function createExtensionPingMessage() {
        var message = {
            type : "Options_getOption",
            to: "local",
            UBPMessageType: "UBPMessage",
            version : "v1",
            options: {
                name: "ubp_root"
            }
        };
        message.id = uuid();
        message.createTime = Date.now();
        return message;
    }

    /**
     * Function that will execute a postMessage to the AA extension. This will either
     * trigger a response or not. If it does then amazonAssistantListener will be called
     * and if there is no response, then this function will attempt again until the
     * remaining retries is 0. If retries reaches 0 and no message is received from the
     * extension we can assume that the user has not installed AA.
     *
     * @param retries {number}
     */
    function detectAmazonAssistant(retries) {
        window.postMessage(createExtensionPingMessage(), "*");
        setTimeout(function() {
            if (!aaInstalled && retries > 0) {
                detectAmazonAssistant(retries - 1);
            }
        }, aaDetectionInterval);
    }

    /**
     * Add event listener that will alert the page that AA is enabled. This gets
     * called after detectAmazonAssistant fires a UBP event to the extension if
     * there is a browser installed.
     *
     * @param response {object}
     */
    function amazonAssistantListener(response) {
        if (response.data && response.data.UBPMessageType === "UBPMessageResponse") {
            aaInstalled = true;
            updateAAInstallWidget();
        }
    }

    /**
     * Update the widget with installed messaging to let the user know that they have
     * already installed the extension.
     */
    function updateAAInstallWidget() {
        var installedButton = document.getElementById("aa-widget-already-installed");
        var splitButtonContainer = document.getElementById("aa-widget-desktop-container");
        if (installedButton && splitButtonContainer) {
            splitButtonContainer.classList.add("no-display");
            installedButton.classList.remove("no-display");
        } else {
            console.error("AA is installed, but was unable to find button containers");
        }
    }

    /**
     * Returns the status of the desktop extension installation
     * @returns {boolean}
     */
    function isAAInstalled() {
        return aaInstalled;
    }

    /**
     * Add the AA listener to the DOM and kick off the workflow once the page loads
     */
    P.when("A", "ready").execute(function (A) {
        /* Add listener for when the workflow sends a request and get a UBP response */
        window.addEventListener("message", amazonAssistantListener, false);

        /* Begin the detection workflow */
        detectAmazonAssistant(aaDetectionRetries);
    });

    return {isAAInstalled};
}());
</script>

