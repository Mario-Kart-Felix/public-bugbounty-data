<script>
(function() {
    /**
     * Initializes the dropdown menu with event listeners to redirect the user when the select an option
     */
    P.when("A", "ready").execute(function (A) {
        /* Whether this browser is officially supported */
        var isSupported = false;

        /* The name of the current browser */
        var browser = "UNSUPPORTED";

        /* Endpoint used to send install ping metrics */
        var preInstallEndpoint = "/gp/ubp/misc/ping_pages/pre_install_ping.html";

        /* Tag base to use when sending the pre install ping */
        var tagBase = "amazon1";

        /* Ref tag used for attribution */
        var ref = "bit_preinstall_undef_i";

        var $ = A.$;
        var mainButton = $(".aa-install-desktop");

        /**
         * Event listener used to redirect users to the browser install URLs
         */
        function onButtonClick(e) {
            sendPreInstallPing();
            if (e && e.target && e.target.value && isSupported && !aaExtensionDetection.isAAInstalled()) {
                window.location.href = e.target.value;
            }
        }

        /**
         * Sends a ping to the install ping endpoint for recording install metrics. This function
         * can be replaced with CSM once onboarding is complete since this ping sends a dataOnly call
         * which is not recorded in Clickstream tables, and dataOnly calls are on path for deprecation by
         * June 2020. This ping will still send operational click metrics which is why it was still included.
         */
        function sendPreInstallPing() {
            var isSmile = window.location.hostname.includes("smile");
            var bitMode = isSmile ? "smile" : "org";
            var version = browser.toLowerCase();
            version = version.charAt(0).toUpperCase() + version.substring(1);
            var endpoint = preInstallEndpoint + "?ref=" + ref + "&tagbase=" + tagBase
                + "&bitMode=" + bitMode + "&version=" + version;
            $.ajax({
                type    : "POST",
                url     : endpoint,
                async   : false
            });
        }

        $(mainButton).click(onButtonClick);
    });
})();
</script>

