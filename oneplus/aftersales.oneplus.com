    .cookie-warn{
      position:relative;
      background: #52565a;
      text-align: center;
      font-size: 14px;
      height: 50px;
      overflow: hidden;
    }
    .cookie-warn-container{
      position: relative;
      max-width: 1200px;
      min-width: 300px;
      display: block;
      margin: auto;
      width: 100%;
    }
    .cookie-warn-content{
      display: block;
      padding-left: 50px;
      padding-right: 50px;
    }
    .cookie-notice__desc {
      line-height: 50px;
      font-size: 14px;
      color: #cacaca;
      word-break: break-word;
    }
    .cookie-notice__link {
      display: inline-block;
      font-size: 14px;
      color: #fff;
    }
    .close-cookie-warn{
      display: block;
      width: 50px;
      height: 50px;
      position: absolute;
      top: 0;
      right: 0;
    }
    .close-cookie-warn:hover{
      cursor: pointer;
    }
    .close-cookie-warn::before,.close-cookie-warn::after{
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);
      content: "";
      display: block;
      background-color: #fff;
    }
    .close-cookie-warn::before{
      border-top: 1px solid #fff;
      border-bottom: 1px solid #fff;
      height: 0;
      width: 16px;
      position: absolute;
      left: 50%;
      top: 50%;
      margin-top: -1px;
      margin-left: -8px;

    }
    .close-cookie-warn::after{
      border-left: 1px solid #fff;
      border-right: 1px solid #fff;
      height: 16px;
      width: 0;
      position: absolute;
      left: 50%;
      top: 50%;
      margin-left: -1px;
      margin-top: -8px;
    }
    .close-cookie-warn:hover:after, .close-cookie-warn:hover:before{
      background-color: #fff;
      border-color: #ccc;
    }
    #header .family-show .show-window span {
      line-height: 1.5;
    }
    @media screen and (max-width: 1050px){
      .cookie-warn{
        height: 50px;
      }
      .cookie-notice__desc{
        line-height: 25px;
      }
      #header .family-show .show-window span {
