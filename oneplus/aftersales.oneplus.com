                      <div class="cart-link" v-for="storeItem in cart.storeLinks">
                        <one-button class="full-width btn-black btn-ghost" type="link" :href="storeItem.href ? storeItem.href : 'https://www.oneplus.com/store'">{{storeItem.label}}</one-button>
                      </div>
                    </div>
                    <div v-show="!(headerIsXs && cart.storeLinks.length)">
                      <div class="cart-link">
                        <one-button class="full-width btn-black btn-ghost" ref="bagLink" type="link" href="https://www.oneplus.com/store">Go shopping</one-button>
                      </div>
                    </div>
                  </div>
                  <div class="cart-link" v-if="cart.showError && !cart.loading">
                    <one-button class="full-width" ref="bagLink" type="link" href="https://www.oneplus.com/jcart">Check my shopping cart</one-button>
                  </div>
                  <div class="cart-link" v-if="(!cart.showError && !isEmptyCart) && !cart.loading">
