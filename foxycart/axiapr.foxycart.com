<script>
    $(document).ready(function(){

$('.select-option').find('a').click(function(){
            var linkto = $(this).attr('href');
            $('html, body').animate({
                    scrollTop: $(linkto).offset().top - 121
            }, 600);
            return false;
        });

        $('#optionform').submit(function(e){
            var selectedVal = $(this).find('select').find('option:selected').val();

            if(selectedVal == '' || selectedVal.length <= 0) {
                alert('Please select a package.')
                return false;
                e.preventDefault();
            }
        });

        $('#optionform').find('select').change(function(){
            var selPrice = $(this).find('option:selected').attr('data-price');
            $('#optionform').find('.package-price').val(selPrice);
        });

        $('.checkbox-options').find('input[type="checkbox"]').change(function(){
            var checkedBox = $(this).is(':checked');

            if(checkedBox) {
                var price = $(this).attr('data-value');
                var num = $(this).attr('data-number');

                $(this).prev('input[name="'+num+':price"]').val(price);
            }
            else {
                var price = $(this).attr('data-value');
                var num = $(this).attr('data-number');

                $(this).prev('input[name="'+num+':price"]').val('');
            }
        });

        $("a.foxy-button").attr('href', function(i,a){
            var d = new Date();
            var m = d.getMonth()+1 < 10 ? 0+""+(d.getMonth()+1):d.getMonth()+1;
            var day = d.getDate() < 10 ? 0+""+d.getDate():d.getDate();
            var strDate = (d.getFullYear()+1) +"-"+ m +"-"+ day; console.log(strDate)
                return a.replace( /(contract_end_date=)[0-9]+/ig, '$1'+strDate );
        });

        $('a.submitForm').click(function(e){
            $('form#foxyform').submit();

            return false;
            e.preventDefault();
        });

        $('select[name="number_of_locations"]').change(function(){
            var number_of_locations = $(this).find('option:selected').val();

            var initial_fee = $('input[name="2:price"]').attr('data-min') * number_of_locations;
            var monthly_fee = $('input[name="price"]').attr('data-min') * number_of_locations;

            $('.initial_fee').text(initial_fee);
            $('.monthly_fee').text(monthly_fee);
            var format_initial = $('.initial_fee').text().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");
            var format_month = $('.monthly_fee').text().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,");

            $('input[name="2:price"]').val(initial_fee);
            $('input[name="price"]').val(monthly_fee);

            $('.initial_fee').text(format_initial);
            $('.monthly_fee').text(format_month);
        });

        var d = new Date();
        var new_date = new Date(d.getFullYear(), d.getMonth()+6, d.getDate());
        var m = new_date.getMonth()+1 < 10 ? 0+""+(new_date.getMonth()+1):new_date.getMonth()+1;
        var day = new_date.getDate() < 10 ? 0+""+new_date.getDate():new_date.getDate();
        var strDate = (new_date.getFullYear()) +"-"+ m +"-"+ day;

        $('input[name="contract_end_date"]').val(strDate);
    });
</script>

