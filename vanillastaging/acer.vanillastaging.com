<style>
/* Lower Search Bar on homepage */
.isHomepage .c_Frame-content .SearchBox {
    margin-bottom: 18px;
    position: relative;
    top: 56px;
}
/* Remove home page banner text */
.isHomepage .c_Hero-title {
    visibility: hidden;
}

/* Add yellow highlight to Accepted answers  */
body.Section-Discussion .c_Content .Item.QnA-Item-Accepted,
body.Section-Discussion .c_Content .Item.QnA-Item-Accepted:hover {
    background-color: #ffffcc !important;
}

/* Remove activity form and comments link */
body.Section-ActivityList .ActivityFormWrap .FormWrapper,
body.Section-ActivityList .Activities .MItem.AddComment,
body.Section-Profile .DataListWrap .FormWrapper,
body.Section-Profile .Activities .MItem.AddComment {
    display: none;
}
body.Section-Profile .DataList.Activities {
    border-top: none;
}

/* Online Count */
.Box.WhosOnline .Count { background-color: #616161; }

/* Recent module tabs */
.c_Recents-tabTitle {
    border: 1px solid #ddd;
    border-bottom: none;
    border-radius: 10px 10px 0 0;
}

/* 3701: remove user role on discussion pages */
body.Section-Discussion .AuthorInfo .RoleTitle { 
    display: none;
}

/* 5821 Predator gaming time on home page */
.isHomepage a.c_PromotedCategories-category.bgPredator { background-size: cover;
    background-image: url(https://static.acer.com/up/Resource/Acer/Community/Predator/20181204/predator-tile-blue.jpg);
}
.isHomepage a.c_PromotedCategories-category.bgPredator.isPredatorCategory:before { background-image: none; }
.isHomepage a.c_PromotedCategories-category.bgPredator.isPredatorCategory { flex-direction: unset !important; }
.isHomepage a.c_PromotedCategories-category.bgPredator h3 { color: #bababa; }
.isHomepage a.c_PromotedCategories-category.bgPredator h3:hover { color: #fff;  /* 80c342; */ }


/* EN-only footer column */
body.langES .c_Footer-column.col-Resources,
body.langDE .c_Footer-column.col-Resources,
body.langPT .c_Footer-column.col-Resources { display: none; }

/* Social icons in footer */
.c_Footer-nav .acer-social { padding: 0; }
.c_Footer-nav .acer-social a { margin-left: 0.5rem; margin-right: 0.5rem; display:inline-block; width: 32px; height: 32px; overflow: hidden;}
.c_Footer-nav .acer-social a span { height: inherit; display: block; background-size: 32px auto; }
/*
_:-ms-fullscreen, :root .c_Footer-nav .acer-social a span { height: inherit; display: block; background-size: 64px auto; background-position-x: -16px; }
_:-ms-fullscreen, :root .c_Footer-nav .acer-social a:hover span { height: inherit; display: block; background-size: 64px auto; background-position-x: -16px; }
*/
.c_Footer-nav .acer-social a span.facebook { background-image: url(//static.acer.com/up/Resource/Acer/Community/icons/20180510/socialmedia_facebook.svg); }
.c_Footer-nav .acer-social a span.twitter { background-image: url(//static.acer.com/up/Resource/Acer/Community/icons/20180510/socialmedia_twitter.svg); }
.c_Footer-nav .acer-social a span.youtube { background-image: url(//static.acer.com/up/Resource/Acer/Community/icons/20180510/socialmedia_youtube.svg); }
.c_Footer-nav .acer-social a span.instagram { background-image: url(//static.acer.com/up/Resource/Acer/Community/icons/20180510/socialmedia_instagram.svg); }
.c_Footer-nav .acer-social a span:hover { background-position: 0 -32px; }

/* OCS banners */
.osc-banner-container { display: none; }
.osc-banner-container.active { display: block; }
.osc-banner-container.horizontal { text-align: center; }
.osc-banner-container.vertical { margin-bottom:15px; }
.osc-banner-container.horizontal img,
.osc-banner-container.vertical img { max-width:100%; }

/* TEST - 4607 */
.CommentsWrap .MessageList { margin-bottom: 20px; }

/* 5642 */
@font-face {
	font-family: TTSquares;
	font-weight: normal;
	src: url('//fonts.acer.com/TypeType_TTSquares_Regular_0.eot');
	src: url('//fonts.acer.com/TypeType_TTSquares_Regular_0.woff') format('woff'),
		 url('//fonts.acer.com/TypeType_TTSquares_Regular_0.ttf') format('truetype');
}
.c_Promos-promo--predator * {
    font-family: TTSquares,Arial,sans-serif !important;
}
body.Section-Predator .c_PreFooter-text {
    font-size: 10px;
}
/* 5689 */
.QuickSearch.Open .MenuItems { min-width: 300px; }
.QuickSearch.Open #Form_search { color: #414141; font-weight: normal; }

/* 5812 */
div.Popup { top: 65vh !important; }
div.Popup a.Close { right: 35px; font-size: 30px; }

/* 5876 */
@media screen and (max-width: 700px) {
    .Flyout.Flyout.Flyout {
        botton: auto;
        top: 0;
    }
}
/* 6097 promo box */
.c_Promos-promo.c_Promos-promo--marketing {
    padding: 0;
    border: none;
    /*background-color: #fafafa;*/
    background: url('https://static.acer.com/up/Resource/Acer/Community/Promos/20191008/GoodDesign_background.jpg') repeat-x;
}
.c_Promos-promo.c_Promos-promo--marketing a {
    display: inline-grid;
}
.c_Promos-promo.c_Promos-promo--marketing img {
    margin: 0 auto;
}

.c_PromotedCategories-title { font-weight: bold; }

/* 6152 */
@media screen and (max-width: 700px) {
   .Flyout.Flyout.Flyout, .ButtonGroup .Dropdown { top: 180px; }
}
/* 6227 */
.InfoMessage { background: none; border: none; padding: 0; }
.InfoMessage img { max-width: 100%; }

/* TEMP */
body.Section-Predator.PredatorDen .Box.WhosOnline, 
body.Section-Predator.PredatorDen #WhosOnline.Box { overflow-y: auto !important;}

</style>
